{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "images/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(create_payment|execute_payment|debug|success|cancel)",
      "methods": ["GET", "POST"],
      "dest": "app.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/Purchasepage.html",
      "dest": "Purchasepage.html"
    },
    {
      "src": "/index.html",
      "dest": "index.html"
    },
    {
      "src": "/pricing.html",
      "dest": "pricing.html"
    },
    {
      "src": "/contact.html",
      "dest": "contact.html"
    },
    {
      "src": "/TOS_SUCH.html",
      "dest": "TOS_SUCH.html"
    },
    {
      "src": "/receipt.html",
      "dest": "receipt.html"
    },
    {
      "src": "/images/(.*)",
      "dest": "images/$1"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ]
}